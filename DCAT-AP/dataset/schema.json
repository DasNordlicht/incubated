{
  "$schema": "http://json-schema.org/schema#",
  "$schemaVersion": "0.0",
  "$id": "",
  "title": "Distribution DCAT-AP 2.0 schema",
  "description": "Distribution Schema meeting DCAT-AP 2.0 specification",
  "type": "object",
  "properties": {
    "type": {
      "type": "string",
      "enum": [
        "Dataset"
      ],
      "description": "Property. It has to be Dataset"
    },
    "title": {
      "type": "array",
      "description": "Property. This property contains a name given to the Dataset. This property can be repeated for parallel language versions of the name.",
      "items": {
        "type": "string"
      }
    },
    "contactPoint": {
      "type": "array",
      "description": "Property. Model:'vcard:Kind'. This property contains contact information that can be used for sending comments about the Dataset.",
      "items": {
        "type": "string"
      }
    },
    "distribution": {
      "type": "array",
      "description": "Property. This property links the Dataset to an available Distribution. Model:'dcat:distribution'",
      "items": {
        "type": "string",
        "format": "uri"
      }
    },
    "keyword": {
      "type": "array",
      "description": "Property. This property contains a keyword or tag, describing the Dataset. Model:'dcat:keyword'",
      "items": {
        "type": "string"
      }
    },
    "publisher": {
      "description": "Property. Model:'foaf:Agent'. This property refers to an entity (organisation) responsible for making the Dataset available",
      "type": "string"
    },
    "spatial": {
      "description": "Geoproperty. Model:'dct:Location'. This property refers to a geographic region that is covered by the Dataset",
      "type": "array",
      "items": {
        "$ref": "http://geojson.org/schema/Geometry.json#"
      }
    },
    "temporal": {
      "description": "Property. This property refers to a temporal period that the Dataset covers. Model:'dct:PeriodOfTime'",
      "type": "array",
      "items": {
        "type": "string",
        "format": "date-time"
      }
    },
    "theme": {
      "description": "Property. Model:'dcat:theme'. This property refers to a category of the Dataset. A Dataset may be associated with multiple themes",
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "accessRights": {
      "type": "string",
      "description": "Property. Model:'foaf:Agent'. This property refers to information that indicates whether the Dataset is open data, has access restrictions or is not public. A controlled vocabulary with three members (:public, :restricted, :non-public) will be created and maintained by the Publications Office of the EU. Enum:'public, restricted, non-public'",
      "enum": [
        "public",
        "restricted",
        "non-public"
      ]
    },
    "creator": {
      "type": "string",
      "description": "Property. Model:'https://schema.org/Text'. This property refers to the entity primarily responsible for producing the catalogue"
    },
    "documentation": {
      "description": "foaf:Document",
      "type": "array",
      "items": {
        "type": "string",
        "format": "uri"
      }
    },
    "frequency": {
      "type": "string",
      "description": "dct:Frequency"
    },
    "hasVersion": {
      "description": "dcat:Dataset",
      "type": "array",
      "items": {
        "type": "string",
        "format": "uri"
      }
    },
    "identifier": {
      "type": "array",
      "items": {
        "type": "string",
        "format": "uri"
      }
    },
    "isReferencedBy": {
      "type": "array",
      "items": {
        "type": "string",
        "format": "uri"
      }
    },
    "isVersionOf": {
      "type": "array",
      "items": {
        "type": "string",
        "format": "uri"
      }
    },
    "landingPage": {
      "description": "foaf:Document",
      "type": "array",
      "items": {
        "type": "string",
        "format": "uri"
      }
    },
    "language": {
      "description": "dct:LinguisticSystem",
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "otherIdentifier": {
      "type": "array",
      "items": {
        "type": "string",
        "format": "uri"
      }
    },
    "provenance": {
      "description": "dct:ProvenanceStatement",
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "qualifiedAttribution": {
      "description": "prov:qualifiedAttribution",
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "qualifiedRelation": {
      "description": "dcat:Relationship",
      "type": "array",
      "items": {
        "type": "string",
        "format": "uri"
      }
    },
    "relation": {
      "description": "rdfs:Resource",
      "type": "array",
      "items": {
        "type": "string",
        "format": "uri"
      }
    },
    "releaseDate": {
      "type": "string",
      "format": "date-time"
    },
    "sample": {
      "description": "rdfs:Resource",
      "type": "array",
      "items": {
        "type": "string",
        "format": "uri"
      }
    },
    "source": {
      "description": "rdfs:Resource",
      "type": "array",
      "items": {
        "type": "string",
        "format": "uri"
      }
    },
    "spatialResolution": {
      "description": "xsd:decimal",
      "type": "array",
      "items": {
        "type": "number"
      }
    },
    "temporalResolution": {
      "description": "xsd:duration",
      "type": "array",
      "items": {
        "type": "number"
      }
    },
    "Type": {
      "description": "skos:Concept",
      "type": "string"
    },
    "modified": {
      "description": "rdfs:Literal typed as xsd:date or xsd:dateTime",
      "type": "string",
      "format": "date-time"
    },
    "versionInfo": {
      "type": "string"
    },
    "versionNotes": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "wasGeneratedBy": {
      "description": "prov:Activity",
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "required": [
    "id",
    "type",
    "description",
    "title"
  ]
}
