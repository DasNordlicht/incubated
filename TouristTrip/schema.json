          "type": "array",
          "items": {
            "type": "string",
            "enum": [
              "taxi",
              "uber",
              "vtc"
            ]
          }
        },
        "electricTransport": {
          "type": "array",
          "items": {
            "type": "string",
            "enum": [
              "electricCar",
              "electricBicycle",
              "electricMotorBike",
              "electricScooter"
            ]
          }
        }
